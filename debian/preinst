#!/bin/sh

set -e

case "$1" in
    install|upgrade)
    
        # If user had manually installed apt1 scripts, we need to rename them
        if [ -d /opt/bro/share/bro/site/apt1 ]; then
                mv /opt/bro/share/bro/site/apt1 /opt/bro/share/bro/site/apt1.2.1
        fi

    ;;

    abort-upgrade)
    ;;

    *)
        echo "preinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
